    "babel-preset-react": "^6.24.1",
    "chokidar": "^2.0.2",
    "fb-watchman": "^2.0.0",
    "rollup": "^0.56.0",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^3.0.3",
