{
  "name": "@hubspot/canvas",
  "version": "0.0.4",
  "description": "HubSpot's Canvas Design System",
  "main": "dist/canvas.js",
  "browser": "dist/canvas.umd.js",
  "module": "dist/canvas.esm.js",
  "repository": "git@github.com:HubSpot/canvas.git",
  "license": "MIT",
  "scripts": {
